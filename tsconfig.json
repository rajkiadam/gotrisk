{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "player": [
        "dist/player/player",
        "dist/player"
      ],
      "money": [
        "dist/money/money",
        "dist/money"
      ],
      "unit": [
        "dist/unit/unit",
        "dist/unit"
      ],
      "map": [
        "dist/map/map",
        "dist/map"
      ],
      "area": [
        "dist/area/area",
        "dist/area"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}